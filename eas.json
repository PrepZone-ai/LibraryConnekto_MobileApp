{
  "cli": {
    "version": ">= 16.3.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    },
    "local": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "custom": {
      "android": {
        "image": "ubuntu-22.04-android-33-ndk-r25c",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentials": true,
        "buildType": "apk",
        "env": {
          "ANDROID_NDK_HOME": "/opt/android/ndk/25.2.9519653"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
